package trainres.beans;

public class PassengerBean {

	private int p_id;
	private String p_name;
	
	public PassengerBean(int p_id, String name) {
		super();
		this.p_id = p_id;
		this.name = name;
	}
	public int getP_id() {
		return p_id;
	}
	public void setP_id(int p_id) {
		this.p_id = p_id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}

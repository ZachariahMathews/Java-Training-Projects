package jstlDemo.beans;

public class Person {

	private String name;
	private int salary;
	
	public Person() {
		// TODO Auto-generated constructor stub
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSalary() {
		return salary;
	}

	public Person(String name, int salary) {
		super();
		this.name = name;
		this.salary = salary;
	}

	public void setSalary(int salary) {
		this.salary = salary;
	}
	
}
